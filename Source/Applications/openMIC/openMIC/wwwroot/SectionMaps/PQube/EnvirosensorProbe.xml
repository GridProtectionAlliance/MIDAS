	<PROPDEF NAME="probeBUndertemperatureEvent" ALIAS="Undertemperature Event" TYPE="CHECKBOX" DEFAULT="OFF" MAP=".Probe_B_Undertemperature_Events" TRUE="ON" FALSE="OFF"/>
