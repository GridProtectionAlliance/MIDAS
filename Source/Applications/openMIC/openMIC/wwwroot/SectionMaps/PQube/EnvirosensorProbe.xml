	<PROPDEF NAME="probeALowHumidityThreshold" ALIAS="Low Humidity Threshold" TYPE="NUMERIC" DEFAULT="5" MAP=".Probe_A_Low_Humidity_Threshold_in_Percent_RH"/>
	<PROPDEF NAME="probeAHumidityEventHysteresis" ALIAS="Humidity Event Hysteresis" TYPE="NUMERIC" DEFAULT="2" MAP=".Probe_A_Humidity_Event_Hysteresis_in_Percent_RH"/>
