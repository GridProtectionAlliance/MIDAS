<SECTION NAME="dualVoltageMode" ALIAS="Dual Voltage Mode" MAPROOT="Dual_Voltage_Measurement">
	<PROPDEF ALIAS="Setup" TYPE="HEADER"/>
	<PROPDEF NAME="dualVoltageSetupMode" ALIAS="Dual Voltage Setup Mode" TYPE="CHECKBOX" DEFAULT="OFF" MAP=".Dual_Voltage_Mode" TRUE="ON" FALSE="OFF" POSTREAD="toggleDualPower('{sectionName}', {value})" POSTWRITE="$('#{sectionName}_dualPowerSetupMode').prop('disabled', {value} === 'OFF'); $('#{sectionName}_voltageMainsLabel').prop('disabled', {value} === 'OFF')"/>
	<PROPDEF NAME="dualPowerSetupMode" ALIAS="Dual Power Setup Mode" CONFIG="3e" TYPE="CHECKBOX" DEFAULT="OFF" MAP=".Dual_Power_Mode" TRUE="ON" FALSE="OFF"/>
	<PROPDEF NAME="vatModuleConnected" ALIAS="VAT Module Connected" TYPE="SELECT" DEFAULT="None" MAP="">
		<PARAM TYPE="TEXT">
			<LIST>
				<ITEM NAME="None" VALUE="NONE"/>
				<ITEM NAME="VAT1-600" VALUE="VAT1-600"/>
				<ITEM NAME="VAT2-600_Current" VALUE="VAT2-600_Current"/>
				<ITEM NAME="VAT2-600_Analog" VALUE="VAT2-600_Analog"/>
				<ITEM NAME="VAT2-020_Current" VALUE="VAT2-020_Current"/>
				<ITEM NAME="VAT2-020_Analog" VALUE="VAT2-020_Analog"/>
			</LIST>
		</PARAM>
	</PROPDEF>
	<PROPDEF ALIAS="Parameters" TYPE="HEADER"/>
	<PROPDEF NAME="voltageMainsLabel" ALIAS="Voltage Mains Label" TYPE="TEXT" DEFAULT="Upstream Voltage" MAP=".Voltage_Mains_Label"/>
	<PROPDEF NAME="dualVoltageLabel" ALIAS="Dual Voltage Label" TYPE="TEXT" DEFAULT="Downstream Voltage" MAP=""/>
	<PROPDEF NAME="nominalPhasetoNeutralV" ALIAS="Nominal Phase to Neutral Voltage" TYPE="NUMERIC" DEFAULT="120" MAP="">
		<PARAM MIN="1"/>
	</PROPDEF>
	<PROPDEF NAME="voltagePTRatio" ALIAS="Voltage PT Ratio" TYPE="TEXT" DEFAULT="1:1" MAP=""/>
	<PROPDEF NAME="neutralVRatio" ALIAS="Neutral Voltage Ratio" TYPE="TEXT" DEFAULT="1:1" MAP=""/>
	<PROPDEF NAME="voltageRange" ALIAS="Voltage Range" TYPE="SELECT" DEFAULT="HIGH" MAP="">
		<PARAM TYPE="TEXT">
		  <LIST>
			<ITEM NAME="HIGH" VALUE="HIGH"/>
			<ITEM NAME="LOW" VALUE="LOW"/>
		  </LIST>
		</PARAM>
	</PROPDEF>  
	<PROPDEF NAME="neutralVoltageRange" ALIAS="Neutral Voltage Range" TYPE="SELECT" DEFAULT="HIGH" MAP="">
		<PARAM TYPE="TEXT">
		  <LIST>
			<ITEM NAME="HIGH" VALUE="HIGH"/>
			<ITEM NAME="LOW" VALUE="LOW"/>
		  </LIST>
		</PARAM>
	</PROPDEF>  
	<PROPDEF NAME="dualVoltageCablingConfiguration" ALIAS="Dual Voltage Cabling Configuration" TYPE="SELECT" DEFAULT="Wye" MAP="">
		<PARAM TYPE="TEXT">
		  <LIST>
			<ITEM NAME="Wye" VALUE="Wye"/>
			<ITEM NAME="Delta" VALUE="Delta"/>
		    <ITEM NAME="Single_Phase_L1_N" VALUE="Single_Phase_L1_N"/>
			<ITEM NAME="2_Differential_Channels" VALUE="2_Differential_Channels"/>
		  </LIST>
		</PARAM>
	</PROPDEF>  
	<PROPDEF ALIAS="Parameters" TYPE="HEADER" MAPROOT=""/>
</SECTION>
