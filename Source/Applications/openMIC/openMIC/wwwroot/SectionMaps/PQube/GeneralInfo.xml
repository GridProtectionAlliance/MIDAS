		<PARAM MIN="3" MAX="30"/>
	</PROPDEF>
	<PROPDEF NAME="upsBatteryPacks" ALIAS="Number Of Battery Packs" TYPE="SELECT" DEFAULT="1" MAP=".UPS_MODEL">
