	<PROPDEF NAME="enableDig1Events" ALIAS="Enable DIG1 Events" TYPE="CHECKBOX" DEFAULT="OFF" MAP=".Enable_DIG1_Events" TRUE="ON" FALSE="OFF" POSTREAD="toggleDig1('{sectionName}', {value})" POSTWRITE="toggleDig1('{sectionName}', {value})"/>
	<PROPDEF NAME="dig1NormalState" ALIAS="DIG1 Normal State" TYPE="SELECT" DEFAULT="NC" MAP=".DIG1_Normal_State" READ="removeQuotes({mapExpr})" WRITE="addQuotes({value})">
